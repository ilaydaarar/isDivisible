using System;

class Solution
{
    static Boolean isDivisible(long number){

        long tmp = number;
        int sum = 0;
        while(number != 0){
            int x = (int) number % 10;
            sum += x;
            number /= 10;
        }

        if(tmp % sum == 0) return true;
        else return false;
    }
    static void Main(string[] args)
    {
        long number = 481;
        Console.WriteLine(isDivisible(number));

    }
}
